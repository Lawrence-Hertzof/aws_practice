{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowS3ObjectsReadWriteDelete",
            "Effect": "Allow",
            "Action": [
              "s3:PutObject",
              "s3:GetObject",
              "s3:GetObjectTagging",
              "s3:DeleteObject",              
              "s3:DeleteObjectVersion",
              "s3:GetObjectVersion",
              "s3:GetObjectVersionTagging",
              "s3:GetObjectACL",
              "s3:PutObjectACL"
            ],
            "Resource": ["arn:aws:s3:::env0files0processflow/"]
        }
    ]
}

